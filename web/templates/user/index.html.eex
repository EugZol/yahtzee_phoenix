<h2>Listing users</h2>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Score</th>
    </tr>
  </thead>
  <tbody>
    <%= for user <- @users do %>
      <tr>
        <td><%= link user.name, to: user_path(@conn, :show, user) %></td>
        <td><%= user_score(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
